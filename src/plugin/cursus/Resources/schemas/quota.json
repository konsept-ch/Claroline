{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "The quota uuid"
    },
    "organization": {
      "type": "object",
      "properties": {
        "uuid": {
          "type": "string",
          "description": "The quota organization"
        }
      }
    },
    "options": {
      "type": "object",
      "properties": {
        "default": {
          "type": "object",
          "description": "The default quota"
        },
        "years": {
          "type": "object",
          "description": "The years quota"
        }
      }
    }
  },
  "required": ["organization", "options"],
  "claroline": {
    "ids": ["id"]
  }
}
